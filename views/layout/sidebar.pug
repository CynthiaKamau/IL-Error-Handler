						| Realtime Logs
					span.pull-right-container

			li
				a(href='/appointments')
					i.fa.fa-th
					span
						| Appointments
					span.pull-right-container
						
			li
				a(href='/clients')
					i.fa.fa-th
					span
						| Clients
					span.pull-right-container

			li
				a(href='/observations')
					i.fa.fa-th
					span
						| Observations
					span.pull-right-container

					
						
			
				
				